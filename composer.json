{
    "name": "mwi/laravel-permissions-sync",
    "description": "Quickly sync permissions",
    "keywords": ["laravel", "permissions", "sync", "spatie"],
    "license": "MIT",
    "authors": [
        {
            "name": "Midwestern Interactive",
            "email": "support@buildmidwestern.com"
        }
    ],
    "type": "library",
    "require": {
        "illuminate/support": "^5.8"
    },
    "autoload": {
        "psr-4": { "MWI\\LaravelDBSync\\": "src/" }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MWI\\LaravelPermissionsSync\\ServiceProvider"
            ]
        }
    }
}
